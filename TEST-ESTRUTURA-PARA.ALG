Algoritmo "test-estrutura-para"



Var
    N, i, x, soma : inteiro


Inicio

      escreva("Quanto número serão digitados: ")
      leia(N)

      soma <- 0 // a soma sempre deve iniciar valor 0.

      para i de 1 ate N faca

      escreva("Digite um número: ")
      leia(x)

      soma <- soma + x

      fimpara

      escreval(" SOMA = ", soma)


Fimalgoritmo